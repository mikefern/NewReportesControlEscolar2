    private void Text32_BeforePrint(object sender, EventArgs e)
    {
      string espe =  Report.GetColumnValue(&quot;fndListaAlumnosOrd.Descripcion&quot;).ToString();
      string sexoEncargado =  Report.GetColumnValue(&quot;fndListaAlumnosOrd.Sexo&quot;).ToString();
      
      if (espe.ToUpper() == &quot;PREPARATORIA&quot; )
        texto2 = &quot;Director&quot;;
      else if (espe == &quot;Medicina General&quot; || espe == &quot;Médico General&quot;)
      {
        if (Report.GetColumnValue(&quot;fndListaAlumnosOrd.ID_Campus&quot;).ToString() == &quot;01&quot;)
          texto2 = &quot;Director de la Facultad de Medicina&quot;;
        else
          texto2 = &quot;Director de la Escuela de Medicina&quot;;
      }
      else if (Report.GetColumnValue(&quot;fndListaAlumnosOrd.ID_Campus&quot;).ToString() == &quot;11&quot; || 
        Report.GetColumnValue(&quot;fndListaAlumnosOrd.ID_Campus&quot;).ToString() == &quot;07&quot;)
        texto2 = &quot;Coordinador de Carrera&quot;;
      else if (Report.GetColumnValue(&quot;fndListaAlumnosOrd.ID_Campus&quot;).ToString() == &quot;55&quot;)
        texto2 = &quot;Secretario Académico&quot;;
      else
      {
        if(sexoEncargado == &quot;M&quot;) 
          texto2 = &quot;Director de Carrera&quot;;
        else 
          texto2 = &quot;Directora de Carrera&quot;;
      }
      
    }

    private void Text33_BeforePrint(object sender, EventArgs e)
    {
      string espe =  Report.GetColumnValue(&quot;fndListaAlumnosOrd.Descripcion&quot;).ToString();
      string campust = Report.GetColumnValue(&quot;fndListaAlumnosOrd.ID_Campus&quot;).ToString();
      
      if (campust == &quot;08&quot;)
        controles = &quot;Coordinación de Control Escolar&quot;;
      else if (campust == &quot;07&quot; &amp;&amp; espe.Contains(&quot;Medicina&quot;))
        controles = &quot;Secretario Académico General&quot;;
      else if (campust == &quot;07&quot;)
        controles = &quot;Secretario Académico&quot;;
      else if (campust == &quot;11&quot; || campust == &quot;15&quot;)
        controles = &quot;Responsable de Control Escolar&quot;;
      else if (campust == &quot;32&quot;)
        controles = &quot;Encargada de Control Escolar&quot;;
      else if (campust == &quot;55&quot;)
        controles = &quot;Director de Campus&quot;;
      else
        controles = &quot;Coordinación de Control Escolar&quot;;
    }

